<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">


	<!-- Servlets -->
	
	<servlet>
		<servlet-name>addCourse</servlet-name>
		<servlet-class>com.lynch.strokesgained.AddCourseServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>addCourse</servlet-name>
		<url-pattern>/addcourse</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addPlayer</servlet-name>
		<servlet-class>com.lynch.strokesgained.AddPlayerServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>addPlayer</servlet-name>
		<url-pattern>/addplayer</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addRound</servlet-name>
		<servlet-class>com.lynch.strokesgained.AddRoundServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>addRound</servlet-name>
		<url-pattern>/addround</url-pattern>
	</servlet-mapping>
	
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>addround.jsp</welcome-file>
	</welcome-file-list>

</web-app>